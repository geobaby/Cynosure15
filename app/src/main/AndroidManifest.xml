                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ReachUsActivity"
            android:label="@string/title_activity_reach_us" />
        <activity
            android:name=".QuestionsActivity"
            android:label="@string/title_activity_questions" />
        <activity
            android:name=".RulesandRegulationsActivity"
            android:label="@string/title_activity_rulesand_regulations">
